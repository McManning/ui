{
  "name": "@oris/ui",
  "version": "4.0.0",
  "description": "React UI components for ORIS applications",
  "author": "Chase McManning <mcmanning.1@osu.edu>",
  "private": true,
  "main": "dist/components/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-rainbow-components": "^1.7.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "npx styleguidist server",
    "build:styleguide": "npx styleguidist build",
    "build:components": "npx babel ./src --out-dir ./dist",
    "build:copy-sass": "node ./scripts/copy-sass.js",
    "build:copy-ts": "node ./scripts/copy-ts.js",
    "build": "npm-run-all --parallel build:components build:styleguide --serial build:copy-sass build:copy-ts",
    "test": "react-scripts test"
  },
  "//scripts": {
    "start": "Run styleguidist server for local development of ORIS/UI",
    "styleguide": "Build a deployable styleguide for upload to webservers",
    "build": "Compile ES6 code to deployable source distributions via Babel",
    "test": "Something that doesn't work yet"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-transform-typescript": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.6.0",
    "acorn": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "react-dropzone": "^10.1.8",
    "react-styleguidist": "^9.2"
  },
  "//devDependencies": {
    "@babel/*": "Build dist/* files for usage through npm imports",
    "acorn": "Forced upgraded version to help resolved a bug in Styleguidist",
    "npm-run-all": "Parallel/Serial process runner for npm run commands",
    "react-dropzone": "Dependency of Files component",
    "react-styleguidist": "Styleguide build and local development support"
  }
}
